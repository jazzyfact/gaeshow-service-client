{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "prettier", "plugin:import/errors", "plugin:import/warnings"],
    "rules": {
        "no-console": "off",
        "no-case-declarations": "off"
    },
    "settings": {
        "import/resolver": {
            "babel-module": {
                "paths": ["./public"]
            }
        }
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "parser": "babel-eslint"
}
